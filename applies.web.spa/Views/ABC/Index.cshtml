                        <tr ng-repeat="(row,raw) in TableMeta.sos.data" ng-init="c = TableMeta.sos.cols;sonbr=raw[c.indexOf('SO_NBR')]" ng-class="{'success':chosenso.SO_NBR==sonbr}">
