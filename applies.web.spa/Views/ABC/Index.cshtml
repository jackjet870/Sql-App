                            <tr>
                                <th colspan="2">订单</th>
                                <td colspan="3">{{chosenco.CO_NBR}}</td>
                                <th colspan="3">一次性费用</th>
                                <td colspan="4">{{chosenco.ONE_FEE}}</td>
                            </tr>
                            <tr>
                                <th colspan="2">受理:</th>
                                <td colspan="5"><pre>{{'受理人:' + chosenco.combo || (chosenco.SO_STAFF_NAME +'\n营业厅:'+ chosenco.WORKAREA + '-'+chosenco.SO_METH) }}</pre></td>
                                <th colspan="2">商品</th>
